/include/ "../skeleton.dtsi"

&pm8941_l3 {
	regulator-min-microvolt = <1000000>;
	regulator-max-microvolt = <1200000>;
	qcom,init-voltage = <1000000>;
};

&pm8941_torch {
	qcom,current = <90>;
};

&soc {
	qcom,cci@fda0C000 {
		actuator0: qcom,actuator@18 {
			cell-index = <1>;
			reg = <0x18>;
			compatible = "qcom,actuator";
			qcom,cci-master = <0>;
		};

		eeprom0: qcom,eeprom@a6 {
			cell-index = <0>;
			reg = <0xa6 0x0>;
			qcom,eeprom-name = "sonyimx135";
			compatible = "qcom,eeprom";
			qcom,slave-addr = <0xa6>;
			qcom,cci-master = <0>;
			qcom,eprom-name = "sonyimx135";
			qcom,num-blocks = <1>;
			qcom,page0 = <0 0x0 2 0x0 1 20>;
			qcom,poll0 = <0 0x0 2 0x0 1 20>;
			qcom,mem0 = <0x800 0x0000 2 0 1 0>; //EEPROM READ

			cam_vdig-supply = <&pm8941_l3>;
			qcom,cam-vreg-name = "cam_vdig", "cam_vio", "cam_vana", "cam_vaf";
			qcom,cam-vreg-type = <0 1 1 1 >;
			qcom,cam-vreg-min-voltage = <1050000 0 0 1800000>;
			qcom,cam-vreg-max-voltage = <1050000 0 0 1800000>;
			qcom,cam-vreg-op-mode = <105000 0 0 100000>;
			qcom,gpio-no-mux = <0>;
			gpios = <&msmgpio 15 0>, 		// MCLK
				<&msmgpio 4 0>,			// MAIN_CAM_RESET
				<&msmgpio 16 0>,			// 13M_VANA_EN
				<&msmgpio 96 0>,			// 13M_VIO_EN
				<&msmgpio 145 0>,			// 13M_VCM_EN : OIS_MVDD
				<&msmgpio 30 0>,		// OIS_LDO_EN : OIS_VDD
				<&msmgpio 29 0>,		// OIS_RESET
				<&pm8941_gpios 1 0>;	// LDAF_EN
			qcom,gpio-reset = <1>; 		// Set gpio_4 as reset
			qcom,gpio-vana = <2>;		// Set gpio_16 as VANA
			qcom,gpio-vio = <3>;			// Set gpio_96 as VIO
			qcom,gpio-vaf = <4>;			// Set pm8941_gpio_4 as VCM
			qcom,gpio-ois-ldo = <5>;		// Set gpio_145 as OIS_LDO_EN
			qcom,gpio-ois-reset = <6>;		// Set gpio_29 as OIS_RESET
			qcom,gpio-ldaf-en = <7>;		// Set gpio_29 as OIS_RESET
			qcom,gpio-req-tbl-num = <0 1 2 3 4 5 6 7>;
			qcom,gpio-req-tbl-flags = <0 0 0 0 0 0 0 0>;
			qcom,gpio-req-tbl-label =  "CAMIF_MCLK", "CAM_RESET1", "MAIN_VANA_EN", "MAIN_VIO_EN", "MAIN_VCM_EN",
										"OIS_LDO_EN", "OIS_RESET", "LDAF_EN";

			qcom,cam-power-seq-type = "sensor_gpio";	// VIO
			qcom,cam-power-seq-val = "sensor_gpio_vio";
			qcom,cam-power-seq-cfg-val = <1>;
			qcom,cam-power-seq-delay = <20>;
		};

		qcom,camera_kddi@34 {
			status = "ok";
			compatible = "qcom,imx091";
			revision = "rev_a";
			reg = <0x34 0x0>;
			qcom,slave-id = <0x34 0x0 0x091>;
			qcom,csiphy-sd-index = <0>;
			qcom,csid-sd-index = <0>;
//			qcom,actuator-src = <&actuator0>;
//			qcom,eeprom-src = <&eeprom0>;
			qcom,led-flash-src = <&led_flash0>;
			qcom,mount-angle = <90>;
			qcom,sensor-name = "imx091";
			cam_vdig-supply = <&pm8941_l3>;
			qcom,cam-vreg-name = "cam_vdig";
			qcom,cam-vreg-type = <0>;
			qcom,cam-vreg-min-voltage = <1200000>;
			qcom,cam-vreg-max-voltage = <1200000>;
			qcom,cam-vreg-op-mode = <200000>;
			qcom,gpio-no-mux = <0>;
			gpios = <&msmgpio 15 0>, 	// MCLK
				<&msmgpio 4 0>,			// MAIN_CAM_RESET
				<&msmgpio 16 0>,		// 13M_VANA_EN
				<&msmgpio 96 0>,		// 13M_VIO_EN
				<&msmgpio 57 0>;		// 13M_VCM_EN
			qcom,gpio-reset = <1>; 		// Set gpio_4 as reset
			qcom,gpio-vana = <2>;		// Set gpio_16 as VANA
			qcom,gpio-vio = <3>;		// Set gpio_96 as VIO
			qcom,gpio-vaf = <4>;		// Set gpio_57 as VCM
			qcom,gpio-req-tbl-num = <0 1 2 3 4>;
			qcom,gpio-req-tbl-flags = <0 0 0 0 0>;
			qcom,gpio-req-tbl-label =  "CAMIF_MCLK", "CAM0_RESET", "CAM0_VANA_EN", "CAM0_VIO_EN", "CAM0_VCM_EN";
			qcom,csi-lane-assign = <0x4320>;
			qcom,csi-lane-mask = <0x1F>;
			qcom,sensor-position = <0>;
			qcom,sensor-mode = <0>;
			qcom,cci-master = <0>;
		};

		qcom,camera@20 {
			status = "ok";
			compatible = "qcom,imx135";
			revision = "rev_a...";
			reg = <0x20 0x0>;
			qcom,slave-id = <0x20 0x16 0x135>;
			qcom,csiphy-sd-index = <0>;
			qcom,csid-sd-index = <0>;
			qcom,actuator-src = <&actuator0>;
			qcom,eeprom-src = <&eeprom0>;
			qcom,led-flash-src = <&led_flash0>;
			qcom,mount-angle = <90>;
			qcom,sensor-name = "imx135";
			cam_vdig-supply = <&pm8941_l3>;
			qcom,cam-vreg-name = "cam_vdig";
			qcom,cam-vreg-type = <0>;
			qcom,cam-vreg-min-voltage = <1050000>;
			qcom,cam-vreg-max-voltage = <1050000>;
			qcom,cam-vreg-op-mode = <105000>;
			qcom,gpio-no-mux = <0>;
			gpios = <&msmgpio 15 0>,
				<&msmgpio 4 0>,
				<&msmgpio 16 0>,
				<&msmgpio 96 0>,
				<&msmgpio 57 0>;
			qcom,gpio-reset = <1>;
			qcom,gpio-vana = <2>;
			qcom,gpio-vio = <3>;
			qcom,gpio-vaf = <4>;
			qcom,gpio-req-tbl-num = <0 1 2 3 4>;
			qcom,gpio-req-tbl-flags = <0 0 0 0 0>;
			qcom,gpio-req-tbl-label =  "CAMIF_MCLK", "CAM_RESET1", "MAIN_VANA_EN", "MAIN_VIO_EN", "MAIN_VCM_EN";
			qcom,csi-lane-assign = <0x4320>;
			qcom,csi-lane-mask = <0x1F>;
			qcom,sensor-position = <0>;
			qcom,sensor-mode = <0>;
			qcom,cci-master = <0>;
		};

		qcom,camera_kddi@6e {
			status = "ok";
			compatible = "qcom,imx119";
			revision = "rev_a...";
			reg = <0x6e 0x0>;
			qcom,slave-id = <0x6e 0x0 0x119>;
			qcom,csiphy-sd-index = <2>;
			qcom,csid-sd-index = <2>;
			qcom,flash-type = <0>;
			qcom,mount-angle = <270>;
			qcom,sensor-name = "imx119";
			cam_vio-supply = <&pm8941_lvs2>;
			cam_vana-supply = <&pm8941_l18>;
			qcom,cam-vreg-name = "cam_vdig", "cam_vio", "cam_vana";
			qcom,cam-vreg-type = <0 1 0>;
			qcom,cam-vreg-min-voltage = <0 0 2700000>;
			qcom,cam-vreg-max-voltage = <0 0 2700000>;
			qcom,cam-vreg-op-mode = <0 0 200000>;
			qcom,gpio-no-mux = <0>;
			gpios = <&msmgpio 17 0>, //VT_CAM_MCLK
				<&msmgpio 18 0>, //VT_CAM_RESET
				<&msmgpio 30 0>; //VT_LDO_EN
			qcom,gpio-reset = <1>;
			qcom,gpio-vdig = <2>;
			qcom,gpio-req-tbl-num = <0 1 2>;
			qcom,gpio-req-tbl-flags = <0 0 0>;
			qcom,gpio-req-tbl-label = "CAMIF_MCLK2",
						     "CAM1_RESET",
						     "CAM1_VDIG_LDO_EN";
			qcom,csi-lane-assign = <0x4320>;
			qcom,csi-lane-mask = <0x3>;
			qcom,sensor-position = <1>;
			qcom,sensor-mode = <1>;
			qcom,cci-master = <1>;
		};
	};
};
