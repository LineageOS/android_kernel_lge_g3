&pm8941_bms {
	status = "disabled";
};

&pm8941_mpps {
	mpp@a500 { /* MPP 6 */
		status = "ok";
		qcom,mode = <4>;
		qcom,invert = <0>;
		qcom,output-type = <0>;
		qcom,vin-sel = <2>;
		qcom,src-sel = <0>;
		qcom,ain-route = <1>;
		qcom,master-en = <1>;
	};

	mpp@a700 { /* MPP 8 */
		qcom,ain-route = <0x0>;
		qcom,invert = <0x1>;
		qcom,master-en = <0x1>;
		qcom,mode = <0x4>;
		qcom,pin-num = <0x8>;
		qcom,src-sel = <0x0>;
		reg = <0xa400 0x100>;
		status = "ok";
	};
};

&rpm_bus {
	rpm-regulator-ldoa3 {
		pm8941_l3: regulator-l3 {
			regulator-min-microvolt = <1000000>;
			regulator-max-microvolt = <1200000>;
			qcom,init-voltage = <1000000>;
		};
	};

	rpm-regulator-ldoa19 {
		pm8941_l19: regulator-l19 {
			regulator-min-microvolt = <3000000>;
			regulator-max-microvolt = <3000000>;
			qcom,init-voltage = <3000000>;
		};
	};

};
